import csv

dados = [
    ["Nome", "Idade", "Cidade"],
    ["João", 30, "São Paulo"],
    ["Maria", 25, "Recife"],
    ["Ana", 35, "Fortaleza"]
]

with open('dados.csv', 'w', newline='', encoding='utf-8') as arquivo:
    writer = csv.writer(arquivo)
    writer.writerows(dados)

print("Arquivo CSV criado com sucesso.")
